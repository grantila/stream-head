{
  "name": "stream-head",
  "description": "Peek the first couple of bytes from a stream",
  "license": "MIT",
  "version": "1.1.0",
  "author": "Gustaf Räntilä <g.rantila@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/grantila/stream-head.git"
  },
  "types": "./dist/index.d.ts",
  "main": "./es6.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "test": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --bail --check-leaks dist-es6/test dist-es5/test",
    "test-nocov": "./node_modules/.bin/mocha dist-es6/test dist-es5/test",
    "buildtest": "npm run build && npm run test"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "keywords": [
    "stream",
    "head",
    "peek",
    "promise"
  ],
  "dependencies": {
    "through2": "2.x"
  },
  "devDependencies": {
    "@types/mocha": "2.x",
    "@types/node": "8.x",
    "chai": "4.x",
    "coveralls": "2.x",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.x",
    "pre-commit": "1.x",
    "sinon": "4.x",
    "typescript": "2.5.x"
  }
}
